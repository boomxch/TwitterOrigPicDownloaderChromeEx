{
  "manifest_version": 2,
  "name": "Twitter Original Picture Downloader",
  "description": "You can download the original picture in twitter by right clicking at the picture.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon/icon38.png"
  },
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "background": {
    "scripts": [ "js/background.js" ],
    "persistent": true
  },
  "permissions": [
    "contextMenus",
    "downloads",
    "tabs"
  ]
}